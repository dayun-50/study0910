<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="contacts">
	<insert id="insert">
		insert into contacts values(contacts_seq.nextval,#{name},#{contact})
	</insert>

	<delete id="deleteBySeq">
		delete from contacts where seq = #{seq}
	</delete>
	
	<update id="update">
		update contacts set name = #{name} , contact = #{contact} where seq = #{seq}
	</update>
	
	<select id="selectAll" resultType="com.kedu.dto.ContactDTO">
		select * from contacts
	</select>
	
</mapper>