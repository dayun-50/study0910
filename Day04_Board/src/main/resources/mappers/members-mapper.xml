<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

  
  <mapper namespace="Members">
  	<insert id="insert">
  		insert into members values(members_seq.nextval, #{userId}, #{userPw},
  		#{userName}, #{userPhone}, #{userEmail}, #{userZip}, #{userAddress1}, #{userAddress2})
  	</insert>
  
  	<select id="loginGetNickname" resultType="String">
  		select userName from members where userId = #{userId} and userPw = #{userPw}
  	</select>
  </mapper>